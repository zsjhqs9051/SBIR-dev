FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include	"flowCondition"
dimensions      [ 0 2 -2 0 0 0 0 ];

internalField   uniform $k;

boundaryField
{
    Inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    Outlet          
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    ".*"
    {
        type            kqRWallFunction;
        value           $internalField;
    }
	Ceiling
	{
		type            symmetry;
	}
}

// ************************************************************************* //
